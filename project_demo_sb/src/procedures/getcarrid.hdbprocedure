PROCEDURE "GETCARRID"(OUT WHERE_COND NVARCHAR(100)) 

   LANGUAGE SQLSCRIPT
   SQL SECURITY DEFINER
   --DEFAULT SCHEMA <default_schema_name>
   READS SQL DATA AS
BEGIN
DECLARE V_CARRID NVARCHAR(100);
SELECT CONCAT(CONCAT('"CARRID" IN (''', CARRID ),''')') AS CARRID INTO V_CARRID FROM "USER_MAPPING" WHERE "USER" = SESSION_USER;
WHERE_COND:= V_CARRID;
END