PROCEDURE "materialize_scarr"( )
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
  -- READS SQL DATA 
  AS
BEGIN
   
    INSERT INTO T_SCARR	(
    CREATED_DATE,
   	MANDT, 
   	CARRID, 
   	CARRNAME,
   	CURRCODE)
   SELECT
    CURRENT_TIMESTAMP as CREATED_DATE,
    MANDT, 
   	CARRID, 
   	CARRNAME,
   	CURRCODE
   	FROM "cv_SCARR"
   	WHERE URL IS NOT NULL AND MANDT = 300;
END