PROCEDURE "session_client_procedure"(OUT WHERE_COND NVARCHAR(100) )
   LANGUAGE SQLSCRIPT
   SQL SECURITY DEFINER
   --DEFAULT SCHEMA <default_schema_name>
   READS SQL DATA AS
BEGIN
  --DECLARE V_MANDT NVARCHAR(3);
  DECLARE V_COND NVARCHAR(100);
SELECT CONCAT(CONCAT('"MANDT" IN (''', SESSION_CONTEXT('CLIENT') ),''')') AS MANDT INTO V_COND from (SELECT SESSION_CONTEXT('CLIENT') FROM DUMMY);
WHERE_COND:= V_COND;
END